      "enzyme": "^3.11.0",
      "enzyme-adapter-react-16": "^1.15.2",
      "enzyme-to-json": "^3.4.4",
      "file-loader": "^6.0.0",
      "html-webpack-plugin": "^4.3.0",
      "husky": "^4.2.5",
      "identity-obj-proxy": "^3.0.0",
      "jest": "^26.0.1",
      "lint-staged": "^10.2.2",
      "node-sass": "^4.14.1",
      "opn-webpack-plugin": "^1.0.2",
      "postcss-each": "^0.10.0",
      "postcss-import": "^12.0.1",
