      "source-map-loader": "^0.2.4",
      "style-loader": "^1.2.1",
      "ts-essentials": "^6.0.4",
      "tslint": "^6.1.2",
      "tslint-loader": "^3.6.0",
      "tslint-react": "^5.0.0",
      "typescript": "^3.8.3",
      "webpack": "^4.43.0",
      "webpack-cli": "^3.3.11",
      "webpack-dev-server": "^3.11.0",
      "webpack-env": "^0.8.0",
      "webpack-merge": "^4.2.2"
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "*": "npm run prettier"
   },
   "jest": {
      "setupFiles": [
         "./jest/setupTests.js"
      ],
      "snapshotResolver": "./jest/snapshotResolver.js",
      "snapshotSerializers": [
         "enzyme-to-json/serializer"
      ],
      "globals": {
         "__DEV__": true
      },
      "moduleNameMapper": {
         "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/fileMock.js",
         "\\.(css|scss|less)$": "identity-obj-proxy"
      }
   }
}
